<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.natkitv" name="NATKITV REPO" version="1.0.0" provider-name="natki">
    <requires>
        <import addon="xbmc.addon" version="19.0.0"/>
    </requires>
    <extension point="xbmc.addon.repository" name="NATKITV REPO">
        <info>https://natiliza.github.io/natki-tv/zips/repository.natkitv/addon.xml</info>
        <checksum>https://natiliza.github.io/natki-tv/zips/repository.natkitv/addon.xml.md5</checksum>
        <datadir zip="true">https://natiliza.github.io/natki-tv/zips/repository.natkitv/</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>NATKITV repository for Kodi addons</summary>
        <description>Lightweight custom repository for NATKI TV style</description>
        <platform>all</platform>
        <assets>
            <icon>https://natiliza.github.io/natki-tv/zips/repository.natkitv/icon.png</icon>
            <fanart>https://natiliza.github.io/natki-tv/zips/repository.natkitv/fanart.jpg</fanart>
        </assets>
    </extension>
</addon>

<addon id="repository.Fishenzon" name="Fishenzon Repo" version="1.0.0" provider-name="fish">
    <requires>
        <import addon="xbmc.addon" version="19.0.0"/>
    </requires>
    <extension point="xbmc.addon.repository" name="Fishenzon Repo">
        <info>https://natiliza.github.io/natki-tv/zips/repository.Fishenzon/addon.xml</info>
        <checksum>https://natiliza.github.io/natki-tv/zips/repository.Fishenzon/addon.xml.md5</checksum>
        <datadir zip="true">https://natiliza.github.io/natki-tv/zips/repository.Fishenzon/</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Fishenzon repository</summary>
        <description>Addon repository for Fishenzon content</description>
        <platform>all</platform>
    </extension>
</addon>
</addons>