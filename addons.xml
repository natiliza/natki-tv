<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.natkitv" name="NATKITV_REPO" version="1.0.0" provider-name="NatkiTV">
    <requires>
        <import addon="xbmc.addon" version="19.0.0"/>
    </requires>
    <extension point="xbmc.addon.repository" name="NATKITV_REPO">
        <dir>
            <info compressed="false">https://example.com/repository.natkitv/addons.xml</info>
            <checksum>https://example.com/repository.natkitv/addons.xml.md5</checksum>
            <datadir>https://example.com/repository.natkitv/zips/</datadir>
        </dir>
        <info>https://example.com/repository.natkitv/</info>
